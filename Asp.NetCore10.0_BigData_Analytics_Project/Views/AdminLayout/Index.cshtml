
@{
    Layout = null;
}

<!doctype html>
<html class="no-js " lang="en">
@await Component.InvokeAsync("_AdminLayoutHeadComponentPartial") <!--Burada Admin Layout Head Component Kodlarını tuttuk yani css linklerini tuttuk -->
<body class="theme-orange">
    <!-- Page Loader -->
    @await Component.InvokeAsync("_AdminLayoutPageLoaderComponentPartial") <!--Burada Admin Layout Page Loader Component Kodlarını tuttuk yani sayfa yüklenirken gösterilen animasyon kodlarını tuttuk -->
    <!-- Overlay For Sidebars -->
    @await Component.InvokeAsync("_AdminLayoutSearchComponentPartial") <!--Burada Admin Layout Search Component Kodlarını tuttuk yani arama çubuğu kodlarını tuttuk -->
    <!-- Top Bar -->
    @await Component.InvokeAsync("_AdminLayoutNavbarComponentPartial") <!--Burada Admin Layout Navbar Component Kodlarını tuttuk yani üst menü kodlarını tuttuk -->
    <!-- Left Sidebar -->
    @await Component.InvokeAsync("_AdminLayoutSidebarComponentPartial") <!--Burada Admin Layout Sidebar Component Kodlarını tuttuk yani sol menü kodlarını tuttuk-->

    <!-- Right Sidebar -->
   

    <section class="content">
        <div class="block-header">
            <div class="row">
                <div class="col-lg-7 col-md-6 col-sm-12">
                    <h2>
                        Stater Page
                        <small class="text-muted">Welcome to Nexa Application</small>
                    </h2>
                </div>
                <div class="col-lg-5 col-md-6 col-sm-12">
                    <ul class="breadcrumb float-md-right">
                        <li class="breadcrumb-item"><a href="index.html"><i class="zmdi zmdi-home"></i> Nexa</a></li>
                        <li class="breadcrumb-item"><a href="javascript:void(0);">Extra</a></li>
                        <li class="breadcrumb-item active">Stater Page</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row clearfix">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="body">
                            <h4>Stater page</h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Jquery Core Js -->
    <script src="~/nexa/assets/bundles/libscripts.bundle.js"></script> <!-- Lib Scripts Plugin Js -->
    <script src="~/nexa/assets/bundles/vendorscripts.bundle.js"></script> <!-- Lib Scripts Plugin Js -->

    <script src="~/nexa/assets/bundles/mainscripts.bundle.js"></script><!-- Custom Js -->
</body>
</html>